{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",                                 
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false
  },
  "include": ["src/**/*"],
  "exclude": [ "node_modules", "**/*.spec.ts"]
}
